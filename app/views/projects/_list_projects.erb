<div class="card-header text-uppercase"></div>
<% projects.each do |project| %>
  <div class="items-list">
    <div class="il-item">
      <div class="media">
        <div class="media-body">
          <div class="text-muted">
            <div class="row">
              <div class="col-sm-1">
                <%= image_tag(project.image.icon) %>
              </div>
              <div class="col-sm-5">
                <h6 class="media-heading">
                  <%= link_to project.name, project_path(project), :remote => false, :class => 'grey-link' %>
                </h6>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
<% end %>